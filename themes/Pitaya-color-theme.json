{
	"name": "Pitaya",
	"colors": {
		// Editor
		"editor.background": "#1e2124",
		"editor.foreground": "#eeffff",
		"editorLineNumber.dimmedForeground": "#393B41",
		"editorLineNumber.foreground": "#5F626D",
		"editorLineNumber.activeForeground": "#E36397",
		
		// Activity Bar
		"activityBarBadge.background": "#E36397",
		"activityBar.background": "#131416",
		"activityBar.border": "#424549",
		"activityBar.activeBorder": "#E36397",
		"activityBar.activeBackground": "#1e2124",
		"activityBar.activeFocusBorder": "#E36397",
		"activityBarBadge.foreground": "#FFFFFF",

		// Sidebar
		"sideBarTitle.foreground": "#FFFFFF",
		"sideBar.background": "#131416",
		"sideBar.foreground": "#ECEBF3",
		"sideBarSectionHeader.border": "#424549",
		"sideBar.dropBackground": "#1C1E21",
		"sideBar.border": "#424549",
		"sideBarSectionHeader.foreground": "#E36397",

		// Git
		"gitDecoration.untrackedResourceForeground": "#78f070",
		"gitDecoration.modifiedResourceForeground": "#f99f5e",
		"gitDecoration.stageModifiedResourceForeground": "#f99f5e",
		"gitDecoration.addedResourceForeground": "#78f070",
				
		// Focus
		"focusBorder": "#E36397",
		"list.activeSelectionBackground": "#12030B",

		// List
		"list.hoverBackground": "#35222c",
		"list.inactiveSelectionBackground": "#1C1E21",
		"list.inactiveFocusOutline": "#E36397",
		"list.focusAndSelectionOutline": "#E36397",

		// Tab
		"tab.border":"#424549",
		"tab.activeBorder": "#E36397",

		// Status Bar
		"statusBar.background": "#131416",

		// Terminal
		"terminal.foreground": "#ECEBF3",
		"terminalCursor.foreground": "#E36397",

		// Button Control
		"button.background": "#E36397",

		// Text Link
		"textLink.foreground": "#E36397",
		"textLink.activeForeground": "#65DEF1",

		// Badge
		"badge.background": "#E36397",
		"badge.foreground": "#ECEBF3",

		// Progress Bar
		"progressBar.background": "#E36397",

		// Selection
		"selection.background": "#E36397",

		// Input
		"input.background": "#000",
		"input.foreground": "#fff"
	},
	
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#eeffff5d"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#EEFFFF"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#65DEF1"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"constant.other.color",
				"punctuation",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#E36397"
			}
		},
		{
			"name": "Keyword control",
			"scope": [
				"keyword.control",
			],
			"settings": {
				"foreground": "#ff5b9c"
			}
		},
		{
			"name": "Meta tags",
			"scope": ["meta.tag"],
			"settings": {
				"foreground": "#eeffff",
				"fontStyle": ""
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#89C1EA"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#8FE7F5"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FE9920"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#977EF1"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#86DEFE"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#FFC145",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#E36397"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#D64933"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
